text/plain; more %s; copiousoutput; nametemplate=%s.txt
text/html; w3m -I %{charset} -T text/html -F -dump %s; copiousoutput; nametemplate=%s.html
image/*; feh %s
application/msword; wps %s
application/vnd.ms-excel; et '%s'
application/vnd.openxmlformats-officedocument.spreadsheetml.sheet; et '%s'
application/vnd.ms-powerpoint; wpp '%s'
application/vnd.openxmlformats-officedocument.presentationml.presentation; wpp '%s'
application/pdf; evince %s
